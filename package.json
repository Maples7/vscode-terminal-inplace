{
  "name": "terminal-inplace",
  "displayName": "terminal-inplace",
  "description": "Launch your own vscode terminal in the directory of your focus file.",
  "version": "0.0.1",
  "publisher": "Maples7",
  "engines": {
    "vscode": "^1.24.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.launchTerminalInPlace"
  ],
  "main": "./extension",
  "contributes": {
    "keybindings": [
      {
        "command": "extension.launchTerminalInPlace",
        "key": "ctrl+i t",
        "mac": "cmd+i t"
      }
    ],
    "commands": [
      {
        "command": "extension.launchTerminalInPlace",
        "title": "Terminal In Place"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "eslint": "^4.11.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}
